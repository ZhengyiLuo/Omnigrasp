<?xml version="1.0"?>
<robot name="simple_cube">

  <!-- Link definition for the cube -->
  <link name="cube">
    <visual>
      <geometry>
        <box size=".5 .1 .5"/>
      </geometry>
      <material name="color">
        <color rgba="0.9 0.9 0.9 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
        <box size=".5 .1 .5"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.166" ixy="0" ixz="0" iyy="0.166" iyz="0" izz="0.166"/>
    </inertial>
  </link>

</robot>